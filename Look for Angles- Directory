    <script>
        // Navigation functionality
        function showSection(sectionId) {
            // Hide all sections
            document.querySelectorAll('.content-section').forEach(section => {
                section.classList.remove('active');
            });
            
            // Remove active class from all tabs
            document.querySelectorAll('.nav-tab').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Show selected section
            document.getElementById(sectionId).classList.add('active');
            
            // Add active class to corresponding tab
            document.querySelector(`[data-section="${sectionId}"]`).classList.add('active');
        }

        // Tab navigation
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', function() {
                const sectionId = this.dataset.section;
                showSection(sectionId);
            });
        });

        // Revenue Calculator
        function calculateRevenue() {
            const basic = parseInt(document.getElementById('basic-listings').value) || 0;
            const enhanced = parseInt(document.getElementById('enhanced-listings').value) || 0;
            const premium = parseInt(document.getElementById('premium-listings').value) || 0;
            const exclusive = parseInt(document.getElementById('exclusive-partnerships').value) || 0;
            
            const basicRevenue = basic * 75; // Average $75
            const enhancedRevenue = enhanced * 225; // Average $225
            const premiumRevenue = premium * 750; // Average $750
            const exclusiveRevenue = exclusive * 1250; // Average $1,250
            
            const totalRevenue = basicRevenue + enhancedRevenue + premiumRevenue + exclusiveRevenue;
            
            document.getElementById('total-revenue').textContent = '$' + totalRevenue.toLocaleString();
            document.getElementById('revenue-breakdown').innerHTML = `
                <p>Basic Listings: $${basicRevenue.toLocaleString()}</p>
                <p>Enhanced Listings: $${enhancedRevenue.toLocaleString()}</p>
                <p>Premium Listings: $${premiumRevenue.toLocaleString()}</p>
                <p>Exclusive Partnerships: $${exclusiveRevenue.toLocaleString()}</p>
            `;
            document.getElementById('revenue-result').style.display = 'block';
        }

        // Industry Selection
        function selectIndustry(button, industry) {
            // Remove active class from all buttons
            document.querySelectorAll('.industry-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            
            // Add active class to clicked button
            button.classList.add('active');
            
            // Update industry data
            const industryData = {
                'legal': {
                    leadValue: '$100-380',
                    conversionRate: '8-15%',
                    clientValue: '$5K-50K',
                    roiPotential: '400-600%'
                },
                'healthcare': {
                    leadValue: '$118',
                    conversionRate: '10-18%',
                    clientValue: '$500-2K',
                    roiPotential: '350-450%'
                },
                'home-services': {
                    leadValue: '$66',
                    conversionRate: '15-25%',
                    clientValue: '$200-1K',
                    roiPotential: '400-500%'
                },
                'professional': {
                    leadValue: '$100-250',
                    conversionRate: '5-12%',
                    clientValue: '$1K-10K',
                    roiPotential: '300-400%'
                },
                'automotive': {
                    leadValue: '$45-90',
                    conversionRate: '12-20%',
                    clientValue: '$300-800',
                    roiPotential: '250-350%'
                },
                'retail': {
                    leadValue: '$25-50',
                    conversionRate: '20-30%',
                    clientValue: '$50-200',
                    roiPotential: '200-300%'
                }
            };
            
            const data = industryData[industry];
            document.getElementById('lead-value').textContent = data.leadValue;
            document.getElementById('conversion-rate').textContent = data.conversionRate;
            document.getElementById('client-value').textContent = data.clientValue;
            document.getElementById('roi-potential').textContent = data.roiPotential;
        }

        // Geographic Analysis
        function showRegionData(region) {
            const regionData = {
                'premium': {
                    title: 'Premium Urban Markets',
                    pricing: '$200-400/month',
                    description: 'Major metropolitan areas with high competition and premium client values',
                    examples: 'Los Angeles, Portland, Seattle',
                    leadCost: '$150-380 per lead',
                    characteristics: 'High competition, premium pricing, established markets'
                },
                'standard': {
                    title: 'Standard Metro Markets',
                    pricing: '$100-200/month',
                    description: 'Mid-sized cities with moderate competition and good business density',
                    examples: 'Salem, Eugene, Medford',
                    leadCost: '$75-150 per lead',
                    characteristics: 'Moderate competition, balanced pricing, growth markets'
                },
                'competitive': {
                    title: 'Competitive Rural Markets',
                    pricing: '$50-100/month',
                    description: 'Smaller cities with lower competition and market penetration opportunities',
                    examples: 'Grants Pass, Ashland, Corvallis',
                    leadCost: '$35-75 per lead',
                    characteristics: 'Low competition, competitive pricing, high margins'
                }
            };
            
            const data = regionData[region];
            document.getElementById('region-details').innerHTML = `
                <h3>${data.title}</h3>
                <div class="metric-value">${data.pricing}</div>
                <div class="metric-label">Average Monthly Pricing</div>
                <p><strong>Description:</strong> ${data.description}</p>
                <p><strong>Examples:</strong> ${data.examples}</p>
                <p><strong>Lead Cost:</strong> ${data.leadCost}</p>
                <p><strong>Characteristics:</strong> ${data.characteristics}</p>
            `;
            document.getElementById('region-analysis').style.display = 'block';
        }

        // Geographic Revenue Calculator
        function calculateGeographicRevenue() {
            const marketType = document.getElementById('market-type').value;
            const businessCount = parseInt(document.getElementById('business-count').value) || 0;
            
            const marketMultipliers = {
                'premium': 300,
                'standard': 150,
                'competitive': 75
            };
            
            if (marketType && businessCount) {
                const revenue = businessCount * marketMultipliers[marketType];
                document.getElementById('market-revenue').textContent = '$' + revenue.toLocaleString();
                document.getElementById('market-breakdown').innerHTML = `
                    <p>Market Type: ${marketType.charAt(0).toUpperCase() + marketType.slice(1)}</p>
                    <p>Business Count: ${businessCount}</p>
                    <p>Average Revenue per Business: $${marketMultipliers[marketType]}</p>
                `;
                document.getElementById('geographic-result').style.display = 'block';
            } else {
                alert('Please select market type and enter business count');
            }
        }

        // Business ROI Calculator
        function calculateBusinessROI() {
            const businessType = document.getElementById('business-type').value;
            const monthlyInvestment = parseFloat(document.getElementById('monthly-investment').value) || 0;
            const customerValue = parseFloat(document.getElementById('customer-value').value) || 0;
            const monthlyLeads = parseInt(document.getElementById('monthly-leads').value) || 0;
            const conversionRate = parseFloat(document.getElementById('conversion-rate-input').value) || 0;
            
            if (monthlyInvestment && customerValue && monthlyLeads && conversionRate) {
                const conversions = monthlyLeads * (conversionRate / 100);
                const monthlyRevenue = conversions * customerValue;
                const roi = ((monthlyRevenue - monthlyInvestment) / monthlyInvestment) * 100;
                
                document.getElementById('roi-percentage').textContent = roi.toFixed(0) + '%';
                document.getElementById('roi-breakdown').innerHTML = `
                    <p>Monthly Investment: $${monthlyInvestment.toLocaleString()}</p>
                    <p>Monthly Leads: ${monthlyLeads}</p>
                    <p>Conversions: ${conversions.toFixed(1)}</p>
                    <p>Monthly Revenue: $${monthlyRevenue.toLocaleString()}</p>
                    <p>Net Profit: $${(monthlyRevenue - monthlyInvestment).toLocaleString()}</p>
                `;
                document.getElementById('roi-result').style.display = 'block';
            } else {
                alert('Please fill in all fields');
            }
        }

        // Partnership Selection
        function selectPartnership(element, type) {
            // Remove active class from all partnership models
            document.querySelectorAll('.partnership-model').forEach(model => {
                model.classList.remove('active');
            });
            
            // Add active class to selected model
            element.classList.add('active');
            
            const partnershipData = {
                'subscription': {
                    title: 'Subscription Revenue Model',
                    description: 'Predictable monthly recurring revenue from tiered listing subscriptions',
                    advantages: 'Steady cash flow, predictable growth, easy to scale',
                    pricing: 'Basic: $50-100, Enhanced: $150-300, Premium: $500-1000',
                    examples: 'Most successful directories use this as primary revenue model'
                },
                'performance': {
                    title: 'Performance-Based Revenue Model',
                    description: 'Commission-based revenue sharing aligned with business success',
                    advantages: 'Aligned incentives, scalable revenue, long-term partnerships',
                    pricing: '5-10% commission on completed transactions',
                    examples: 'Home services: 5-7%, Professional services: 8-12%'
                },
                'advertising': {
                    title: 'Advertising Revenue Model',
                    description: 'Display advertising and sponsored content revenue streams',
                    advantages: 'Supplementary income, leverages existing traffic, passive revenue',
                    pricing: 'Typically 15-20% of total directory revenue',
                    examples: 'Banner ads: $200-500/month, Sponsored content: $500-2000/month'
                },
                'value-added': {
                    title: 'Value-Added Services Model',
                    description: 'Additional services leveraging existing business relationships',
                    advantages: 'Higher margins, deeper relationships, competitive differentiation',
                    pricing: 'SEO: $500-1500/month, Social media: $300-800/month',
                    examples: 'Website design, marketing services, business consulting'
                }
            };
            
            const data = partnershipData[type];
            document.getElementById('partnership-content').innerHTML = `
                <h3>${data.title}</h3>
                <p><strong>Description:</strong> ${data.description}</p>
                <p><strong>Advantages:</strong> ${data.advantages}</p>
                <p><strong>Pricing:</strong> ${data.pricing}</p>
                <p><strong>Examples:</strong> ${data.examples}</p>
            `;
            document.getElementById('partnership-details').style.display = 'block';
        }

        // Partnership Revenue Calculator
        function calculatePartnershipRevenue() {
            const subscription = parseFloat(document.getElementById('subscription-revenue').value) || 0;
            const commission = parseFloat(document.getElementById('commission-revenue').value) || 0;
            const advertising = parseFloat(document.getElementById('advertising-revenue').value) || 0;
            const services = parseFloat(document.getElementById('services-revenue').value) || 0;
            
            const total = subscription + commission + advertising + services;
            
            document.getElementById('partnership-total').textContent = '$' + total.toLocaleString();
            document.getElementById('partnership-breakdown').innerHTML = `
                <p>Subscription Revenue: $${subscription.toLocaleString()}</p>
                <p>Commission Revenue: $${commission.toLocaleString()}</p>
                <p>Advertising Revenue: $${advertising.toLocaleString()}</p>
                <p>Value-Added Services: $${services.toLocaleString()}</p>
            `;
            document.getElementById('partnership-result').style.display = 'block';
        }

        // Oregon City Data
        function showCityData(city) {
            const cityData = {
                'portland': 'Premium Market: $300-400/month average, 650K+ population',
                'salem': 'Standard Market: $150-250/month average, state capital',
                'eugene': 'Standard Market: $125-200/month average, university town',
                'medford': 'Standard Market: $100-175/month average, regional hub',
                'grants-pass': 'Growth Market: $75-125/month average, tourism focus',
                'ashland': 'Growth Market: $75-150/month average, arts community'
            };
            
            alert(`${city.charAt(0).toUpperCase() + city.slice(1)}: ${cityData[city] || 'Market data available'}`);
        }

        // Oregon Revenue Calculator
        function calculateOregonRevenue() {
            const city = document.getElementById('oregon-city').value;
            const businesses = parseInt(document.getElementById('oregon-businesses').value) || 0;
            
            const cityMultipliers = {
                'portland': 350,
                'salem': 200,
                'eugene': 175,
                'medford': 150,
                'grants-pass': 100,
                'ashland': 125
            };
            
            if (city && businesses) {
                const revenue = businesses * cityMultipliers[city];
                document.getElementById('oregon-monthly').textContent = '$' + revenue.toLocaleString();
                document.getElementById('oregon-breakdown').innerHTML = `
                    <p>City: ${city.charAt(0).toUpperCase() + city.slice(1)}</p>
                    <p>Target Businesses: ${businesses}</p>
                    <p>Average Revenue per Business: $${cityMultipliers[city]}</p>
                `;
                document.getElementById('oregon-result').style.display = 'block';
            } else {
                alert('Please select a city and enter business count');
            }
        }

        // Implementation Calculator
        function calculateImplementation() {
            const targetRevenue = parseFloat(document.getElementById('target-revenue').value) || 0;
            const avgRevenue = parseFloat(document.getElementById('avg-revenue').value) || 0;
            const totalMarket = parseInt(document.getElementById('total-market').value) || 0;
            
            if (targetRevenue && avgRevenue && totalMarket) {
                const requiredBusinesses = targetRevenue / avgRevenue;
                const penetrationRate = (requiredBusinesses / totalMarket) * 100;
                
                document.getElementById('penetration-rate').textContent = penetrationRate.toFixed(1) + '%';
                document.getElementById('implementation-breakdown').innerHTML = `
                    <p>Target Revenue: $${targetRevenue.toLocaleString()}</p>
                    <p>Required Businesses: ${requiredBusinesses.toFixed(0)}</p>
                    <p>Total Market: ${totalMarket.toLocaleString()}</p>
                    <p>Businesses Needed: ${requiredBusinesses.toFixed(0)}</p>
                `;
                document.getElementById('implementation-result').style.display = 'block';
            } else {
                alert('Please fill in all fields');
            }
        }

        // Initialize with welcome message
        document.addEventListener('DOMContentLoaded', function() {
            console.log('💰 Welcome to Directory Monetization Mastery! Ready to build profitable business networks?');
        });
    </script>
</body>
</html>
